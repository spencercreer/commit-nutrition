{
  "name": "meal-planner",
  "version": "1.0.0",
  "description": "An application for planning the macronutrients of meals.",
  "main": "server/server.js",
  "scripts": {
    "start": "nodemon server/server.js",
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencercreer/meal-planner.git"
  },
  "keywords": [],
