  "bugs": {
    "url": "https://github.com/spencercreer/meal-planner/issues"
  },
  "homepage": "https://github.com/spencercreer/meal-planner#readme",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "concurrently": "^7.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
